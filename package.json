{
  "name": "@tigerdata/mcp-boilerplate",
  "version": "0.8.0",
  "description": "MCP boilerplate code for Node.js",
  "license": "Apache-2.0",
  "author": "TigerData",
  "homepage": "https://tigerdata.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/timescale/mcp-boilerplate-node"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./eslintPlugin": {
      "import": "./dist/eslintPlugin.js",
      "types": "./dist/eslintPlugin.d.ts"
    },
    "./instrumentation": {
      "import": "./dist/instrumentation.js",
      "types": "./dist/instrumentation.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.208.0",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "express": "^5.1.0",
    "raw-body": "^3.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.3",
    "@types/express": "^5.0.5",
    "@types/node": "^22.19.1",
    "ai": "^5.0.102",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
